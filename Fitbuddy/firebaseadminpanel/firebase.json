{
  "hosting": {
    "public": "public",
    "rewrites":[
      {
        "source":"/api/send",
        "function":"api"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
